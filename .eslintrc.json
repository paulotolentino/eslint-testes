{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  //   "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double", { "allowTemplateLiterals": true }],
    "react/prop-types": "off",
    "no-undef": "off",
    "no-unused-vars": "warn"
  }
}
